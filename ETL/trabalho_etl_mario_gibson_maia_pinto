    <description/>
    <extended_description>-- Table: stage.stgc_produto

-- DROP TABLE stage.stgc_produto;

CREATE TABLE stage.stgc_produto
(
  cod_produto integer,
  nom_produto character varying(50),
  cod_fornecedor integer,
  cod_setor integer,
  cod_unidade integer,
  vlr_venda numeric(10,2),
  vlr_custo numeric(10,2),
  vlr_medio numeric(10,2),
  cod_promocao integer,
  vlr_promocao numeric(10,2),
  flg_fracionado_ajustado character varying(3),
  car_seq_carga integer
)
WITH (
  OIDS=FALSE
);
ALTER TABLE stage.stgc_produto
  OWNER TO postgres;
</extended_description>
