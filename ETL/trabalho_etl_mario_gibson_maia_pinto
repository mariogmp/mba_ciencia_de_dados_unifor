    <extended_description>-- Table: stage.stgf_produto

-- DROP TABLE stage.stgf_produto;

CREATE TABLE stage.stgf_produto
(
  cod_produto integer,
  nom_produto character varying(50),
  vlr_venda numeric(12,2),
  vlr_custo numeric(12,2),
  vlr_medio numeric(12,2),
  vlr_promocao numeric(12,2),
  flg_fracionado_ajustado character varying(3),
  nom_setor character varying(30),
  nom_unidade character varying(30),
  nom_fornecedor character varying(50),
  cod_fornecedor integer,
  cod_promocao integer,
  car_seq_carga integer
)
WITH (
  OIDS=FALSE
);
ALTER TABLE stage.stgf_produto
  OWNER TO postgres;
</extended_description>
