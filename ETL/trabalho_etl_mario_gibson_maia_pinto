)

1 SQL statements executed

update stage.ctrl_parametro set tip_parametro = 'CARGA';

insert into stage.ctrl_parametro 
select car_seq_carga, dat_carga, 'REPROCESSO'
from stage.ctrl_parametro;</extended_description>
