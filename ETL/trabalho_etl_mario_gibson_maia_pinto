-- DROP TABLE stage.stgc_caixa;

CREATE TABLE stage.stgc_caixa
(
  cod_caixa integer,
  nom_caixa character varying(50),
  cod_loja integer,
  flg_ferias_ajustada character varying(3),
  car_seq_carga integer
)
WITH (
  OIDS=FALSE
);
ALTER TABLE stage.stgc_caixa
  OWNER TO postgres;
