    <description/>
    <extended_description>-- Table: stage.stgc_promocao

-- DROP TABLE stage.stgc_promocao;

CREATE TABLE stage.stgc_promocao
(
  cod_promocao integer,
  nom_promocao character varying(50),
  dat_inicio_vigencia date,
  dat_fim_vigencia date,
  cod_loja integer,
  car_seq_carga integer
)
WITH (
  OIDS=FALSE
);
ALTER TABLE stage.stgc_promocao
  OWNER TO postgres;
</extended_description>
